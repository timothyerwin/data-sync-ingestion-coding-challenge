# Local Development Configuration
# Copy this to .env for local development outside Docker

# Database Configuration (use localhost for local dev)
DATABASE_URL=postgresql://postgres:postgres@localhost:5434/ingestion

# Target API Configuration
API_BASE_URL=http://datasync-dev-alb-101078500.us-east-1.elb.amazonaws.com/api/v1
TARGET_API_KEY=your_api_key_here

# Worker Configuration
WORKER_CONCURRENCY=10
BATCH_SIZE=1000

# Rate limit handling configuration
RATE_LIMIT_BUFFER=5
CURSOR_REFRESH_THRESHOLD=60
